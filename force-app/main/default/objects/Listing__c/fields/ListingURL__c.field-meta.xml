<?xml version="1.0" encoding="UTF-8" ?>
<CustomField xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>ListingURL__c</fullName>
    <description>URL for listing detail</description>
    <formula>IF( $RecordType.Name  ==  &apos;Job&apos; ,
            &apos;https://appexchange.salesforce.com/job?jobId=&apos; + Id,
            &apos;https://appexchange.salesforce.com/listingDetail?listingId=&apos; + Id)</formula>
    <label>Listing URL</label>
    <type>Text</type>
</CustomField>
